{
    "name": "ulrack/cli-edition",
    "description": "The Ulrack CLI Edition project.",
    "keywords": [
        "ulrack",
        "project",
        "cli"
    ],
    "type": "project",
    "license": "MIT",
    "prefer-stable": true,
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "ulrack/cli-application": "^1.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "MyVendor\\MyProject\\": "src/"
        },
        "files": [
            "locator.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MyVendor\\MyProject\\Tests\\": "tests/"
        }
    },
    "archive": {
        "exclude": [
            "/PULL_REQUEST_TEMPLATE.md",
            "/CODE_OF_CONDUCT.md",
            "/CONTRIBUTING.md"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.4"
    }
}
